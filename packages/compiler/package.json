{
  "name": "@nadi.js/compiler",
  "version": "0.1.0-alpha.1",
  "description": "Nadi single-file component compiler",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "nadi",
    "compiler",
    "sfc",
    "single-file-component"
  ],
  "author": "Nadi Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nadijs/nadi",
    "directory": "packages/compiler"
  },
  "homepage": "https://nadijs.org",
  "bugs": "https://github.com/nadijs/nadi/issues",
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.7",
    "@babel/types": "^7.23.6",
    "@babel/generator": "^7.23.6",
    "magic-string": "^0.30.5",
    "postcss": "^8.4.32",
    "postcss-selector-parser": "^6.0.15"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^20.10.6",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1"
  }
}