{
  "name": "@nadi/prerender",
  "version": "0.2.0",
  "description": "Static pre-rendering system for Nadi",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "nadi",
    "prerender",
    "ssg",
    "static-site-generation"
  ],
  "dependencies": {
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "@nadi.js/core": "^0.2.0",
    "@nadi.js/compiler": "^0.2.0",
    "vite": "^5.0.0"
  },
  "author": "Nadi Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nadiframework/nadi.git",
    "directory": "packages/prerender"
  }
}