{
  "name": "@nadi/codemod",
  "version": "0.2.0",
  "description": "Automated migration tools for Nadi framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nadi-codemod": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "nadi",
    "codemod",
    "migration",
    "vue",
    "react",
    "svelte"
  ],
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@babel/generator": "^7.23.6",
    "vue-template-compiler": "^2.7.16",
    "svelte": "^4.2.8",
    "glob": "^10.3.10",
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/babel__traverse": "^7.20.5",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "@nadi/compiler": "^0.2.0"
  },
  "author": "Nadi Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nadiframework/nadi.git",
    "directory": "packages/codemod"
  }
}