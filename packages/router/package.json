{
  "name": "@nadi/router",
  "version": "0.1.0",
  "description": "Lightweight client-side router for Nadi",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "nadi",
    "router",
    "routing",
    "spa",
    "laravel"
  ],
  "author": "Nadi Team",
  "license": "MIT",
  "dependencies": {
    "@nadi/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1"
  },
  "peerDependencies": {
    "@nadi/core": "^0.1.0"
  }
}